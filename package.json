{
  "name": "fullcontact-node",
  "version": "2.0.0",
  "description": "NodeJS SDK for FullContact APIs",
  "main": "lib/fullcontact.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --recursive",
    "docs": "./node_modules/.bin/jsdoc -r ./lib -d docs",
    "docs:md": "./node_modules/.bin/jsdoc2md --files lib/fullcontact.js lib/apis/oauth.js lib/apis/v2/** lib/apis/contacts/** > docs.md",
    "coverage": "./node_modules/istanbul/lib/cli.js cover _mocha -- --recursive -R spec",
    "prepublish": "npm test && npm run docs:md"
  },
  "homepage": "https://github.com/fullcontact/fullcontact-node",
  "repository": {
    "type": "git",
    "url": "git://github.com/fullcontact/fullcontact-node.git"
  },
  "bugs:": "https://github.com/fullcontact/fullcontact-node/issues",
  "keywords": [
    "FullContact",
    "API",
    "SDK"
  ],
  "engines": {
    "node": ">= v6.1.0"
  },
  "author": "FullContact Labs <support+labs@fullcontact.com>",
  "contributors": [
    "Matt Elliott <matt@fullcontact.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "request": "^2.79.0",
    "request-promise": "^4.1.1"
  },
  "devDependencies": {
    "chance": "^1.0.4",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.3",
    "jsdoc-to-markdown": "^3.0.0",
    "mocha": "^3.2.0",
    "nock": "^9.0.6",
    "sinon": "^1.17.7"
  }
}
